    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.0.0",
    "@nestjs/event-emitter": "^2.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^10.0.3",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/websockets": "^10.0.3",
    "@prisma/client": "^6.6.0",
    "@rollup/plugin-typescript": "10.0.1",
    "acorn": "^8.11.3",
    "acorn-typescript": "^1.4.13",
