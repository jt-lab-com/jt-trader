{
  "name": "environment",
  "version": "0.24.10",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build:prod": "nest build && cd client && yarn && yarn build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prestart:prod": "cross-env ENV=production prisma migrate deploy && ts-node scripts/download-markets.ts",
    "proxy:dev": "nest start --watch --entryFile src/data-proxy.js | npx pino-pretty",
    "proxy:prod": "node build/src/data-proxy.js",
    "tester:dev": "NODE_ENV=tester nest start --watch --entryFile console.js | npx pino-pretty",
    "tester:sync:debug": "cross-env NODE_ENV=tester-sync node build/src/console.js",
    "tester:debug": "cross-env NODE_ENV=tester node build/src/console.js",
    "tester:prod": "NODE_ENV=tester node build/src/console.js",
    "prestart:dev": "ts-node scripts/download-markets.ts && prisma migrate dev",
    "start:dev": "nest start --watch | npx pino-pretty",
    "start:inspect" :"node --inspect --expose-gc -r ts-node/register node_modules/@nestjs/cli/bin/nest.js start --watch | npx pino-pretty",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env ENV=production node build/src/main",
    "start:frontend:dev": "cd client && yarn dev",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky"
  },
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.485.0",
