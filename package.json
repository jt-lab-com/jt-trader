    "ioredis": "^5.3.2",
    "logrotator": "^1.1.0",
    "moment": "^2.29.4",
    "nanoid": "3",
    "nest-csv-parser": "^2.0.4",
    "nestjs-pino": "^3.2.0",
    "papaparse": "^5.5.2",
    "pino-http": "^8.3.3",
    "reflect-metadata": "^0.1.13",
